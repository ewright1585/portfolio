  li.nav-item(role='presentation')
    button#define-tab.nav-link(data-bs-toggle='tab' data-bs-target='#define-tab-pane' type='button' role='tab' aria-controls='define-tab-pane' aria-selected='false') Define
  li.nav-item(role='presentation')
    button#ideate-tab.nav-link(data-bs-toggle='tab' data-bs-target='#ideate-tab-pane' type='button' role='tab' aria-controls='ideate-tab-pane' aria-selected='false') Ideate
  li.nav-item(role='presentation')
